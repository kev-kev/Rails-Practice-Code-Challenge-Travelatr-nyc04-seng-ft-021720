<h2>Edit a blogger</h2>
<%= form_for :blogger do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br style = “line-height:40px;”>
  <%= f.label :age %>:
  <%= f.number_field :age %><br style = “line-height:40px;”>
  <%= f.label :bio %>:
  <%= f.text_area :bio %><br style = “line-height:40px;”>

  <%= f.submit %>
<% end %>